{
  "name": "broadcastapp-server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "mocha": "mocha --timeout=60000 --exit",
    "test": "npm run test:unit && npm run test:e2e && npm run coverage",
    "test:unit": "NODE_ENV=test jest --coverage",
    "test:e2e": "NODE_ENV=test npm run fresh && npm run mocha",
    "dev": "cross-env NODE_ENV=development nodemon --inspect=9230 server.js",
    "prettier": "prettier --write --config .prettierrc.json \"**/*.js\"",
    "lint": "eslint --fix --config .eslintrc.json \"**/*.js\"",
    "remark": "remark . -o",
    "coverage": "c8 --all npm run mocha",
    "coverage:report": "c8 report -r html"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": "14"
  },
  "dependencies": {
